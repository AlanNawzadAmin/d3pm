   "source": [
    "import numpy as np\n",
    "from matplotlib import pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "db621f3b-1b86-4cca-8823-2afbd6988b6b",
   "metadata": {},
   "outputs": [],
   "source": [
    "N = 1000\n",
    "M = np.random.randn(N, N) / np.sqrt(N)\n",
    "l = np.linalg.eig(M)[0]\n",
    "plt.figure(figsize=[3, 3])\n",
    "plt.plot(np.real(l), np.imag(l), '.', alpha=0.2)\n",
    "b = 2\n",
    "plt.xlim(-b, b)\n",
    "plt.ylim(-b, b)"
   ]
  },
